{
  "name": "jest-zest",
  "version": "0.2.2",
  "license": "MIT",
  "author": "Patrick Smith",
  "source": "src/index.ts",
  "main": "dist/jest-zest.js",
  "module": "dist/jest-zest.module.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/jest-zest.module.js",
      "import": "./dist/jest-zest.module.js",
      "require": "./dist/jest-zest.js"
    }
  },
  "targets": {
    "main": {
      "optimize": false
    },
    "module": {
      "optimize": false
    }
  },
  "scripts": {
    "prepack": "jest && npm run build",
    "dev": "parcel watch",
    "build": "parcel build",
    "test": "jest --watch"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "jest": "*"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^27.5.1",
    "jest": "^28.1.0",
    "parcel": "^2.6.0",
    "typescript": "^4.7.2"
  }
}
