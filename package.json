{
  "name": "jest-zest",
  "version": "0.3.0",
  "license": "MIT",
  "author": "Patrick Smith",
  "source": "src/index.ts",
  "main": "dist/jest-zest.js",
  "module": "dist/jest-zest.module.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "browser": "./dist/jest-zest.module.js",
      "import": "./dist/jest-zest.module.js",
      "require": "./dist/jest-zest.js"
    }
  },
  "targets": {
    "main": {
      "optimize": false
    },
    "module": {
      "optimize": false
    }
  },
  "scripts": {
    "prepack": "jest && npm run build",
    "dev": "parcel watch",
    "tsc": "tsc --noEmit",
    "build": "parcel build",
    "test": "jest --watch"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "jest": "*"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.10.3",
    "@parcel/transformer-typescript-types": "^2.10.3",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.10",
    "jest": "^29.7.0",
    "parcel": "^2.10.3",
    "typescript": "^4.9.5"
  }
}
